select * 
from analysts_shared.premium_products a
     join
     default.countries b
     on a.country=b.countries_id
where purchase_year_month>='2014-05-01'
      and
      purchase_year_month<='2014-05-31'
      and
      b.countries_iso_code_2 in ('US','CA','AU','UK','NZ');
      
      
      
      
      
